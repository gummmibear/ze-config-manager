{
    "name": "gummmibear/ze-config-manager",
    "description": "Zend Expressive config manager",
    "keywords": [
        "zend",
        "expressive",
        "config"
    ],
    "homepage": "https://github.com/gummmibear/",
    "license": "MIT",
    "authors": [
        {
            "name": "Dawid Jurga",
            "email": "dawidjurga@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/gummmibear/ze-config-manager/issues",
        "source": "https://github.com/gummmibear/ze-config-manager"
    },
    "require": {
        "php": ">=5.4",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mikey179/vfsStream": "~1"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Expressive\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zend\\Expressive\\Config\\Test\\": "test"
        }
    },
    "scripts": {
        "test" : "phpunit test/"
    }
}
